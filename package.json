{
  "name": "vite-vue-electron-ts",
  "version": "1.0.0",
  "main": "src/main/index.ts",
  "scripts": {
    "dev:vue": "vite",
    "build:vue": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "dev:electron": "node script/build.js --watch",
    "build:electron": "node script/build.js",
    "dev:all": "concurrently -n=vue,ele -c=green,blue \"npm run dev:vue\" \"npm run dev:electron\""
  },
  "dependencies": {
    "electron": "^12.0.2",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.37",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "chalk": "^4.1.0",
    "concurrently": "^6.0.1",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.5",
    "rollup-plugin-esbuild": "^3.0.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.3",
    "vue-tsc": "^0.0.8"
  }
}
